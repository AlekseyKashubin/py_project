            userPromise = UserProfile.getUserProfile().done(function (result) {
                user = result;
            });
        },
        updateUserObject: function (userObj) {
            user = userObj;
        },
        isCMS: function () {
            return /^\/cms/.test(window.top.location.pathname);
        },
